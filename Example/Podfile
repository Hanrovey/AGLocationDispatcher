    platform :ios, '7.0'
    pod 'AGLocationDispatcher', :path => "../"
    link_with :AGGeoCodeDemo
    xcodeproj 'AGGeoCodeDemo.xcodeproj'
end

target 'AGLocationDispatcher' do
    platform :ios, '7.0'
    pod 'AGLocationDispatcher', :path => "../"
    link_with :AGLocationDispatcher
    xcodeproj 'AGLocationDispatcher.xcodeproj'
end

target 'AGGeoBackgroundDemo' do
    platform :ios, '7.0'
    pod 'AGLocationDispatcher', :path => "../"
    link_with :AGGeoBackgroundDemo
    xcodeproj 'AGGeoBackgroundDemo.xcodeproj'
end
